function sendLogs(){$.get("/scoring","url="+encodeURIComponent(JSON.stringify(window.location.href)),function(n){function e(){return $('<div class="push-notification"/>').click(function(){$(this).hasClass("push-notification--not-active")&&($(this).removeClass("push-notification--not-active"),$(".push-notification__item").removeClass("hidden push-hide-it").addClass("fadeInRight"))})}function o(n,t){var i=$('<div class="push-notification__item fadeInRight"/>'),r=$("<a/>",{"class":"push-notification__title-link",html:t,href:n,target:"_blank"}).click(function(){u=1}),f=$("<span/>",{"class":"js-push-close"}).click(function(){var n=$(this);setTimeout(function(){$(n).parent().addClass("hidden")},700);$(this).parent().addClass("push-hide-it")});return i.append(r,f),i}function s(){u!==1&&($(".push-notification__item").removeClass("fadeInRight").addClass("push-hide-it"),setTimeout(function(){$(".push-notification__item").addClass("hidden");$(".push-notification").addClass("push-notification--not-active")},700))}var i,t,r,u;n=JSON.parse(n);i=n.suggested_actions.single_url;t=[];for(r in i)t.push(i[r].url[0]);if(console.log(t.length),t.length>0){function f(){for(var r,u,h=$("#maincontainer").append(e()),f=window.location.href,i=f.split("/"),s=i[0]+"//"+i[2],n=0;n<t.length;n++)r=t[n].title,u=s+t[n].value,$(".push-notification").append(o(u,r))}f()}setTimeout(s,25e3)}).fail(function(n){console.log(n+"error")})}window.innerWidth>767&&sendLogs()