function sendLogs(){$.get("/scoring","url="+encodeURIComponent(JSON.stringify(window.location.href)),function(n){function c(){return $('<div class="push-notification"/>').click(function(){$(this).hasClass("push-notification--not-active")&&($(this).removeClass("push-notification--not-active"),$(".push-notification__item").removeClass("hidden push-hide-it").addClass("fadeInRight"))})}function l(n,t){var i=$('<div class="push-notification__item fadeInRight"/>'),r=$("<a/>",{"class":"push-notification__title-link",html:t,href:n,target:"_blank"}).click(function(){e=1}),u=$("<span/>",{"class":"js-push-close"}).click(function(){var n=$(this);setTimeout(function(){$(n).parent().addClass("hidden")},700);$(this).parent().addClass("push-hide-it")});return i.append(r,u),i}function a(){e!==1&&($(".push-notification__item").removeClass("fadeInRight").addClass("push-hide-it"),setTimeout(function(){$(".push-notification__item").addClass("hidden");$(".push-notification").addClass("push-notification--not-active")},700))}function v(){$.get("/scoring/show","utm_scId="+i.utm_scId+"&utm_formId="+i.utm_formId,function(){}).fail(function(){console.log("Error sending stats")})}var u,e;n=JSON.parse(n);var r=n.suggested_actions.single_url,t=[],i={};i.utm_scId=n.uuid;i.utm_formId="1";for(u in r)t.push(r[u].url[0]);var o=window.location.href,f=o.split("/"),s=f[0]+"//"+f[2];if(t.length>0){function h(){for(var f=$("#maincontainer").append(c()),n=0;n<t.length;n++){var r=t[n].title,u=s+t[n].value,i=t[n].value,e=i.substring(i.lastIndexOf("?")+1,i.length);$(".push-notification").append(l(u,r))}}h();v()}setTimeout(a,25e3)}).fail(function(n){console.log(n+" error")})}window.innerWidth>767&&sendLogs()